<script>
     var app = new Vue({
  el: '#app',
  data: {
    fullName:'',
    emailAddress:'',
    password:'',
    rememberMe:true,
    error:'',
    error_signup:''
  },
  methods:{
    login:async function(){
      try{
         let {data} = await axios.put('http://14.160.25.56/api/v1/entrance/login',{emailAddress:this.emailAddress,password:this.password});
         if(data === 'OK') return window.location.href = "/";
    
      }catch(e){
        let _ = this;
        _.error = e.response.data.problems ? e.response.data.problems[0] : e.response.data;
        return setTimeout(function(){
          _.error = '';
        },5000);
      }
    
    },
    register:async function(){
      try{
        let response = await axios.post('http://14.160.25.56/api/v1/entrance/signup',{fullName:this.fullName,emailAddress:this.emailAddress,password:this.password});
         if(response.data === 'OK') return window.location.href = "/";
      }catch(e){
        let _ = this;
        _.error_signup = e.response.data.problems ? e.response.data.problems[0] : e.response.data;
        return setTimeout(function(){
          _.error_signup = '';
        },5000);
      }
        
      
      
      
      
    
    }
  }
})
</script>
